<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.recicla">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_recicla"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/logo_recicla"
        android:supportsRtl="true"
        android:theme="@style/Theme.Recicla">
        <activity
            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".listadoTopten"
            android:exported="false"
            android:parentActivityName=".CalendarEvents" />
        <activity
            android:name=".Estadisticas"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".TopTenAll"
            android:exported="false"
            android:parentActivityName=".Estadisticas" />
        <activity
            android:name=".TopTen"
            android:exported="false" />
        <activity
            android:name=".EventoDetalle"
            android:exported="false" />
        <activity
            android:name=".CalendarEvents"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>